<odoo>
    <!-- Template for Driver Report -->
    <template id="report_tms_driver">
        <t t-call="web.html_container">
            <t t-foreach="doc_ids" t-as="doc">
                <div class="page">
                    <h2>Driver Report</h2>
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>License Number</th>
                                <th>Contact Number</th>
                                <!-- Add other fields as needed -->
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    <t t-esc="doc.name" />
                                </td>
                                <td>
                                    <t t-esc="doc.license_number" />
                                </td>
                                <td>
                                    <t t-esc="doc.contact_number" />
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </t>
        </t>
    </template>

    <!-- Template for Fuel Voucher Report -->
    <template id="report_tms_fuel_voucher">
        <t t-call="web.html_container">
            <t t-foreach="doc_ids" t-as="doc">
                <div class="page">
                    <h2>Fuel Voucher Report</h2>
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>Date</th>
                                <th>Amount</th>
                                <th>Vendor</th>
                                <!-- Add other fields as needed -->
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    <t t-esc="doc.date" />
                                </td>
                                <td>
                                    <t t-esc="doc.amount" />
                                </td>
                                <td>
                                    <t t-esc="doc.vendor_id.name" />
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </t>
        </t>
    </template>

    <!-- Repeat for other models: Travel Expenses, Freight Invoicing, etc. -->
</odoo>