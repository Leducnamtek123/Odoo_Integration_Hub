<odoo>
    <record id="view_places_tree" model="ir.ui.view">
        <field name="name">tms.places.tree</field>
        <field name="model">tms.places</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="address" />
                <field name="latitude" />
                <field name="longitude" />
                <field name="google_maps_url" />
            </tree>
        </field>
    </record>

    <record id="view_places_form" model="ir.ui.view">
        <field name="name">tms.places.form</field>
        <field name="model">tms.places</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="address" />
                        <field name="latitude" />
                        <field name="longitude" />
                        <field name="google_maps_url" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>